#include <stdio.h>

int main() {
    // Write C code here
   int a,b,c;
   scanf("%d %d %d",&a,&b,&c);
   if(a>b){
       if(b>c){
           printf("a is greater");
       }
       else{
             printf("c is greater");
           }
       }
       else if(b>c){
           printf("b is greater");
       }

    return 0;
}
